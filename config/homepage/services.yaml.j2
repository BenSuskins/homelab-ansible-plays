---
# Autogenerated by Ansible
{% set grouped = all_services | groupby('friendly_name') %}

{% for friendly_name, services in grouped %}
- {{ friendly_name }}:
{% for svc in services %}
    - {{ svc.container | capitalize }}:
        href: https://{{ svc.container }}.suskins.co.uk
        icon: {{ svc.container | lower }}
        description: {{ svc.container | capitalize }} container
{% endfor %}
{% endfor %}
###########################
#        General          #
###########################
- General:
{% for svc in static_services %}
    - {{ svc.name | capitalize }}:
        href: https://{{ svc.host }}
        icon: {{ svc.name | lower }}
        description: {{ svc.name | capitalize }}
{% endfor %}
